   "source": [
    "from collections import defaultdict\n",
    "from typing import List\n",
    "\n",
    "strs = [\"eat\", \"tea\", \"ate\", \"nat\", \"bat\"]\n",
    "anagrams = defaultdict(list)\n",
    "\n",
    "for word in strs:\n",
    "    anagrams[''.join(sorted(word))].append(word)\n",
    "print(list(anagrams.values()))\n",
    "\n",
    "\n",
    "\n"
   ],
